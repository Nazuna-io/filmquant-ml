# Secrets and API keys
# These should never be committed to version control
# Copy this file to .env and fill in your actual values

# Application secrets
BORP_APP_SECRET_KEY=your-secret-key-here
BORP_API_API_KEY=your-api-key-here

# Database credentials (if needed in future)
BORP_DB_HOST=localhost
BORP_DB_PORT=5432
BORP_DB_USER=borp_user
BORP_DB_PASSWORD=your-password-here
BORP_DB_NAME=borp_db

# External API keys (examples)
BORP_API_TMDB_KEY=your-tmdb-api-key
BORP_API_GOOGLE_KEY=your-google-api-key

# Cloud storage credentials
BORP_CLOUD_STORAGE_BUCKET=borp-models
BORP_CLOUD_PROJECT_ID=your-project-id
