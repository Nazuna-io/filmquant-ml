# Secrets and API keys
# These should never be committed to version control
# Copy this file to .env and fill in your actual values

# Application secrets
FILMQUANT_ML_APP_SECRET_KEY=your-secret-key-here

# Database credentials (if needed in future)
FILMQUANT_ML_DB_HOST=localhost
FILMQUANT_ML_DB_PORT=5432
FILMQUANT_ML_DB_USER=filmquant_user
FILMQUANT_ML_DB_PASSWORD=your-password-here
FILMQUANT_ML_DB_NAME=filmquant_ml_db

# External API keys
TMDB_API_KEY=your-tmdb-api-key-here
KINOCHECK_API_KEY=your-kinocheck-api-key-here
YOUTUBE_API_KEY=your-youtube-api-key-here

# Cloud storage credentials
FILMQUANT_ML_CLOUD_STORAGE_BUCKET=filmquant-ml-models
FILMQUANT_ML_CLOUD_PROJECT_ID=your-project-id
